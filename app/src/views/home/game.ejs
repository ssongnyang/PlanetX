<!DOCTYPE html>
<html lang="ko">
    <head>
        <meta charset="UTF-8" />
        <meta name="viewport" content="width=device-width, initial-scale=1.0" />
        <title>Space-Themed Board</title>
        <link rel="stylesheet" href="css/game/styles.css" />
    </head>
    <body>
        <div class="board">
            <div class="half-circle half-circle-1">
                <p class="num">1</p>
            </div>
            <div class="half-circle-dummy cover"></div>
            <div class="half-circle half-circle-2">
                <p class="num">2</p>
            </div>
            <div class="half-circle-dummy cover"></div>
            <div class="half-circle half-circle-3">
                <p class="num">3</p>
            </div>
            <div class="half-circle-dummy cover"></div>
            <div class="half-circle half-circle-4">
                <p class="num">4</p>
            </div>
            <div class="half-circle-dummy cover"></div>
            <div class="half-circle half-circle-5">
                <p class="num">5</p>
            </div>
            <div class="half-circle-dummy cover"></div>
            <div class="half-circle half-circle-6">
                <p class="num">6</p>
            </div>
            <div class="half-circle-dummy cover"></div>
            <div class="half-circle half-circle-7">
                <p class="num">7</p>
            </div>
            <div class="half-circle-dummy cover"></div>
            <div class="half-circle half-circle-8">
                <p class="num">8</p>
            </div>
            <div class="half-circle-dummy cover"></div>
            <div class="half-circle half-circle-9">
                <p class="num">9</p>
            </div>
            <div class="half-circle-dummy cover"></div>
            <div class="half-circle half-circle-10">
                <p class="num">10</p>
            </div>
            <div class="half-circle-dummy cover"></div>
            <div class="half-circle half-circle-11">
                <p class="num">11</p>
            </div>
            <div class="half-circle-dummy cover"></div>
            <div class="half-circle half-circle-12">
                <p class="num">12</p>
            </div>
            <div class="half-circle-dummy cover"></div>
            <div class="half-circle-dummy dark-sector"></div>

            <!-- Smaller numbers around the circle -->
            <div class="number-small number-small-1">1</div>
            <div class="number-small number-small-2">2</div>
            <div class="number-small number-small-3">3</div>
            <div class="number-small number-small-4">4</div>
            <div class="number-small number-small-5">5</div>
            <div class="number-small number-small-6">6</div>
            <div class="circle"></div>
        </div>
        <script src="js/game/board.js"></script>
    </body>
</html>
